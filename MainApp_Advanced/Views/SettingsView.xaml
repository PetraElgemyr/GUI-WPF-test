<UserControl x:Class="MainApp_Advanced.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MainApp_Advanced.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="300"
             xmlns:viewmodels="clr-namespace:MainApp_Advanced.ViewModels"
d:DataContext="{d:DesignInstance Type=viewmodels:SettingsViewModel}"
             
             >
    <Grid Background="White">

        <StackPanel Margin="20">
            <TextBlock Text="Settings page" FontSize="20" />
            <Button Content="Gå till home" Margin="0, 10, 0 , 0" Padding="10" Width="auto"  Command="{Binding GoToHomeCommand}"/>
        </StackPanel>

    </Grid>
</UserControl>
